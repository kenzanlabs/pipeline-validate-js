{
  "env": {
    "browser": true,
    "node": true
  },

  "globals": {},

  "plugins": [],

  "rules": {

    "no-case-declarations" : 2,
    "comma-dangle": [2, "never"],
    "no-cond-assign": 2,
    "no-console": 2,
    "no-constant-condition": 2,
    "no-control-regex": 0,
    "no-debugger": 0,
    "no-duplicate-case": 2,
    "no-empty-character-class": 0,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": [2, "all", {
      "conditionalAssign": false,
      "returnAssign": false,
      "nestedBinaryExpressions": false
    }],
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,

    "no-invalid-regexp": 0,
    "no-irregular-whitespace": 0,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 0,
    "no-regex-spaces": 0,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "accessor-pairs": 2,
    "block-scoped-var": 2,
    "complexity": 2,
    "consistent-return": 0,
    "curly": 2,
    "default-case": 2,
    "dot-notation": 2,
    "dot-location": 0,
    "eqeqeq": [2, "allow-null"],
    "guard-for-in": 0,
    "no-alert": 0,
    "no-caller": 0,
    "no-div-regex": 0,
    "no-else-return": 0,
    "no-empty-label": 0,
    "no-eq-null": 0,
    "no-eval": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-redeclare": 2,
    "no-throw-literal": 2,
    "no-warning-comments": [1, { "terms": ["todo", "fixme"], "location": "anywhere" }],
    "radix": [2, "always"],
    "vars-on-top": 2,
    "wrap-iife": [2, "inside"],
    "no-unneeded-ternary" : 2,
    "no-whitespace-before-property": 2,
    "no-implicit-coercion": [2, {
      "boolean": false,
      "number": true,
      "string": true
    }],


    "strict": [2, "global"],


    "no-catch-shadow": 2,
    "no-delete-var": 2,
    "no-unused-vars": 2,


    "handle-callback-err": 2,
    "no-path-concat": 2,
    "no-process-exit": 2,


    "array-bracket-spacing": 2,
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "camelcase": 2,
    "comma-spacing": 2,
    "comma-style": [2, "last"],
    "consistent-this": [0, "self", "that"],
    "eol-last": 0,
    "indent": [2, 2, {
      "VariableDeclarator": 1,
      "SwitchCase": 1
    }],
    "key-spacing": [2, {"beforeColon": false, "afterColon": true}],
    "max-nested-callbacks": [2, {"maximum": 8}],
    "new-cap": 2,
    "new-parens": 2,
    "newline-after-var": 2,
    "no-array-constructor": 2,
    "no-inline-comments": 0,
    "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
    "no-multiple-empty-lines": [2, {"max": 1}],
    "no-new-object": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": 0,
    "no-underscore-dangle": [2, { "allowAfterThis": true }],
    "one-var": [0, {"uninitialized":"always",  "initialized":"always"}],
    "one-var-declaration-per-line": 2,
    "operator-assignment": 0,
    "operator-linebreak": 0,
    "padded-blocks": [2, {"switches":"always"}],
    "quote-props": [2, "consistent"],
    "quotes": [2, "single", "avoid-escape"],
    "semi-spacing": [2, {"before": false, "after": true}],
    "semi": [2, "always"],
    "keyword-spacing": 2,
    "space-before-blocks": 2,
    "space-in-parens": 2,
    "space-infix-ops": [2, {"int32Hint": false}],
    "spaced-comment": [2, "always", {
      "line": {
        "markers": ["/"],
        "exceptions": ["-", "+"]
      },
      "block": {
        "markers": ["!"],
        "exceptions": ["*"]
      }
    }],
    "wrap-regex": 2,
    "newline-per-chained-call": [2, {"ignoreChainWithDepth": 3}],


    "object-curly-spacing": [1, "always", {
      "objectsInObjects": false,
      "arraysInObjects": false
    }],

    "no-bitwise": [2, { "allow": ["~"] }],
    "max-depth": [2, 4],
    "max-len": [2, {"code": 200, "tabWidth": 1, "ignoreUrls": true}],
    "no-plusplus": 2
  }
}